<p id="notice"><%= notice %></p>

<h1>Dailies</h1>

<table>
  <thead>
    <tr>
      <th>Uploader</th>
      <th>Location</th>
      <th>Month</th>
      <th>Day</th>
      <th>Min</th>
      <th>Max</th>
      <th>Precipitation</th>
      <th>Storm</th>
      <th>Shower</th>
      <th>Rain</th>
      <th>Fog</th>
      <th>Sleet</th>
      <th>Snow</th>
      <th>Snow shower</th>
      <th>Snow cover</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dailies.each do |daily| %>
      <tr>
        <td><%= daily.uploader_id %></td>
        <td><%= daily.location_id %></td>
        <td><%= daily.month %></td>
        <td><%= daily.day %></td>
        <td><%= daily.min %></td>
        <td><%= daily.max %></td>
        <td><%= daily.precipitation %></td>
        <td><%= daily.storm %></td>
        <td><%= daily.shower %></td>
        <td><%= daily.rain %></td>
        <td><%= daily.fog %></td>
        <td><%= daily.sleet %></td>
        <td><%= daily.snow %></td>
        <td><%= daily.snow_shower %></td>
        <td><%= daily.snow_cover %></td>
        <td><%= daily.note %></td>
        <td><%= link_to 'Show', daily %></td>
        <td><%= link_to 'Edit', edit_daily_path(daily) %></td>
        <td><%= link_to 'Destroy', daily, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily', new_daily_path %>
